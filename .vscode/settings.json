{
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "editor.fontSize": 12,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "on",
  "editor.autoClosingBrackets": "always",
